# Agentic Search Engine Configuration

# Required: OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# OpenAI Model Settings (optional)
OPENAI_MODEL=gpt-4o-mini
OPENAI_RAG_MODEL=gpt-4o-mini

# Vector Store Path (optional - defaults to Module_3 qdrant_data)
# QDRANT_PATH=./path/to/qdrant_data

# Retrieval Settings (optional)
TOP_K_RESULTS=5

# Semantic Cache Settings (optional)
CACHE_FILE=./semantic_cache.json
CACHE_SIMILARITY_THRESHOLD=0.85

# Web Search Settings (optional)
MAX_SEARCH_RESULTS=5
ENABLE_WEB_SEARCH=true

# Logging (optional)
LOG_LEVEL=INFO
